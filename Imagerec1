import cv2

img=cv2.imread('D:/Images/ImageRec/Copy of real_img.jpg')

img=cv2.cvtColor(img,cv2.COLOR_BGR2GRAY)



img7=cv2.adaptiveThreshold(img,60,cv2.ADAPTIVE_THRESH_MEAN_C,
                              cv2.THRESH_BINARY_INV,5,5)

image8=cv2.adaptiveThreshold(img,60,cv2.ADAPTIVE_THRESH_GAUSSIAN_C,
                             cv2.THRESH_BINARY,5,5)


cv2.imshow("original",img)

cv2.imshow("Gaussian",image8)
cv2.waitKey()
cv2.destroyAllWindows()
